{
  "name": "measure",
  "productName": "measure",
  "version": "0.3.1",
  "description": "Measure values from images",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "start-web": "webpack serve --config ./webpack.web.config.js --mode development",
    "build-web": "webpack --config ./webpack.web.config.js --mode production",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "test": "mocha --require ./babel-register.js './test/**/*.js'"
  },
  "keywords": [],
  "author": {
    "name": "Olav Vatne",
    "email": "olavvatne@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/resources/icon"
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "olavvatne",
              "name": "measure"
            }
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "measure"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/register": "^7.24.6",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-webpack": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^9.1.3",
    "chai": "^4.4.1",
    "css-loader": "^7.1.2",
    "electron": "^30.0.9",
    "favicons": "^7.2.0",
    "favicons-webpack-plugin": "^6.0.1",
    "html-webpack-plugin": "^5.3.2",
    "mocha": "^10.4.0",
    "node-loader": "^2.0.0",
    "sharp": "^0.33.4",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@primer/octicons-react": "^19.9.0",
    "dayjs": "^1.11.11",
    "electron-squirrel-startup": "^1.0.1",
    "exifr": "^7.1.3",
    "moveable-helper": "^0.4.0",
    "papaparse": "^5.4.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-moveable": "^0.30.3",
    "react-router-dom": "^6.23.1",
    "react-table": "^7.8.0",
    "water.css": "^2.1.1"
  },
  "optionalDependencies": {
    "@img/sharp-linux-arm": "^0.33.4",
    "@img/sharp-win32-x64": "^0.33.4"
  }
}
